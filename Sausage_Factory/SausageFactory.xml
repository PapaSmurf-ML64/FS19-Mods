						<fillType name="pig" 		litresPerAnimal="200" />
					</fillTypes>
					<inputMethods>
						<livestockTrigger triggerNode="1|1" />
					</inputMethods>

					<visibilityNodes>
						<nodeGroup hasChildCollisions="true" type="VISIBILITY">
							<child node="fillablePallet1" />
						</nodeGroup>
					</visibilityNodes>
					<digitalDisplays>
						<display displayType="LEVEL" numbers="IN_Display3" emptyNumberColor="1 0 0 1" numberColor="1 1 0 1" fullNumberColor="0 1 0 1"/>
					</digitalDisplays>
					</inputProduct>
				<inputProduct name="IP_SALT" capacity="16000" title="Sausage_Factory_Sausage_Factory_Input2" >
					<fillTypes>
						<fillType name="salt" />
					</fillTypes>

					<inputMethods>
						<unloadingTrigger name="UnloadingTriggerSalt"/>
					</inputMethods>
					<visibilityNodes>
						<nodeGroup hasChildCollisions="true" type="VISIBILITY">
							<child node="fillablePallet2" />
							<child node="fillablePallet3" />
						</nodeGroup>
					</visibilityNodes>

					<digitalDisplays>
						<display displayType="LEVEL" numbers="IN_Display1" emptyNumberColor="1 0 0 1" numberColor="1 1 0 1" fullNumberColor="0 1 0 1"/>
					</digitalDisplays>

				</inputProduct>
				<inputProduct name="IP_EMPTY" capacity="16000" title="Sausage_Factory_Sausage_Factory_Input3" >
					<fillTypes>
						<fillType name="emptypallets" />
					</fillTypes>


					<inputMethods>
						<unloadingTrigger name="UnloadingTriggerEmpty"/>
					</inputMethods>
					<visibilityNodes>
						<nodeGroup hasChildCollisions="true" type="VISIBILITY">
							<child node="fillablePallet4" />
							<child node="fillablePallet5" />
						</nodeGroup>
					</visibilityNodes>

					<digitalDisplays>
						<display displayType="LEVEL" numbers="IN_Display2" emptyNumberColor="1 0 0 1" numberColor="1 1 0 1" fullNumberColor="0 1 0 1"/>
					</digitalDisplays>

				</inputProduct>
			</registerInputProducts>


