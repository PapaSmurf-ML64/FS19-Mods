    <productionFactories>
		<productionFactory indexName="SAUSAGE" saveId="globalCompany_Sausage">
			<guiInformation title="Sausage_Factory_Sausage_FactoryName" imageFilename="store_fermentingSilo.dds" description="Sausage_Factory_Sausage_FactoryDescription" cameraFeed="gui_camera"/>
			<operation updateDelayMinutes="1" showInGlobalGUI="true" />

            <registerUnloadingTriggers>
				<unloadingTrigger name="UnloadingTriggerSalt" exactFillRootNode="exactFillRootNode1" baleTriggerNode="baleTrigger1" />
				<unloadingTrigger name="UnloadingTriggerEmpty" exactFillRootNode="exactFillRootNode2" baleTriggerNode="baleTrigger2" />
			</registerUnloadingTriggers>

			<registerInputProducts>
				<inputProduct name="IP_PIG"  capacity="4000" title="Sausage_Factory_Sausage_Factory_Input1" >
