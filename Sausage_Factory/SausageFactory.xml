<?xml version="1.0" encoding="utf-8" standalone="no" ?>
<placeable>
    <storeData>
        <name>Slaughterhouse</name>
        <specs>
            <capacity>800000</capacity>
            <fillTypes>pig salt emptypallets sausage jambon smokedham</fillTypes>
        </specs>
		<functions>
			<function>his slaughterhouse produces bratwurst, pork chops, and smoked ham.</function>
        </functions>
        <image>store_fermentingSilo.dds</image>
        <price>300000</price>
        <lifetime>2000</lifetime>
        <dailyUpkeep>50</dailyUpkeep>
        <rotation>0</rotation>
        <brand>GCPLACEABLE</brand>
        <species>placeable</species>
        <category>globalCompanyPlaceable</category>
    </storeData>

    <placeableType>GC_ProductionFactoryPlaceable</placeableType>
    <filename>SausageFactory.i3d</filename>
    <placement testSizeX="27" testSizeZ="20" sizeX="27" sizeZ="20" useRandomYRotation="false" useManualYRotation="true" />

	<hotspots>
        <hotspot name="Sausage_Factory" fullName="Slaughterhouse" imageName="SELLING_POINT" hidable="true" linkNode="1" category="CATEGORY_TRIGGERS"/>
    </hotspots>


