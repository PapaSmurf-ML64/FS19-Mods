				<i3dMapping id="fan1" node="17|2|3|0" />
				<i3dMapping id="fan2" node="17|2|4|0" />
				<i3dMapping id="fan3" node="17|2|5|0" />
				<i3dMapping id="fan4" node="17|2|6|0" />
